{
  "openapi": "3.0.0",
  "paths": {
    "/": {
      "get": {
        "operationId": "AppController_getRoot",
        "parameters": [],
        "responses": {
          "200": {
            "description": ""
          }
        },
        "tags": [
          "App"
        ]
      }
    },
    "/auth/register": {
      "post": {
        "operationId": "AuthController_register",
        "parameters": [],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/CreateUserDTO"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Successful registration",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/RegisterResponse"
                }
              }
            }
          }
        },
        "tags": [
          "Auth"
        ]
      }
    },
    "/auth/login": {
      "post": {
        "operationId": "AuthController_login",
        "parameters": [],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/LoginDto"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Successful login",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/LoginResponse"
                }
              }
            }
          }
        },
        "tags": [
          "Auth"
        ]
      }
    },
    "/users/profile": {
      "get": {
        "operationId": "UsersController_getProfile",
        "parameters": [],
        "responses": {
          "200": {
            "description": "Получение профиля пользователя",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/UserDto"
                }
              }
            }
          }
        },
        "tags": [
          "users"
        ]
      }
    }
  },
  "info": {
    "title": "My API",
    "description": "API Documentation",
    "version": "1.0",
    "contact": {}
  },
  "tags": [],
  "servers": [
    {
      "url": "http://localhost:3000"
    }
  ],
  "components": {
    "securitySchemes": {
      "bearer": {
        "scheme": "bearer",
        "bearerFormat": "JWT",
        "type": "http"
      }
    },
    "schemas": {
      "CreateUserDTO": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "description": "Email пользователя"
          },
          "password": {
            "type": "string",
            "description": "Пароль пользователя",
            "minLength": 3
          },
          "firstName": {
            "type": "string",
            "description": "Имя пользователя"
          },
          "lastName": {
            "type": "string",
            "description": "Фамилия пользователя"
          },
          "avatar_url": {
            "type": "string",
            "description": "Аватарка пользователя"
          },
          "profileType": {
            "type": "string",
            "description": "Тип профиля",
            "enum": [
              "employer",
              "job_seeker"
            ]
          }
        },
        "required": [
          "email",
          "password",
          "firstName",
          "lastName",
          "profileType"
        ]
      },
      "UserDto": {
        "type": "object",
        "properties": {
          "id": {
            "type": "number",
            "example": 1,
            "description": "ID пользователя"
          },
          "email": {
            "type": "string",
            "example": "john@example.com",
            "description": "Email пользователя"
          },
          "firstName": {
            "type": "string",
            "example": "John",
            "description": "Имя пользователя"
          },
          "lastName": {
            "type": "string",
            "example": "John",
            "description": "Фамилия пользователя"
          },
          "avatar_url": {
            "type": "string",
            "example": "123-123.png",
            "description": "Аватарка пользователя"
          },
          "profileType": {
            "type": "string",
            "example": "employer",
            "description": "Тип профиля",
            "enum": [
              "employer",
              "job_seeker"
            ]
          }
        },
        "required": [
          "id",
          "email",
          "firstName",
          "lastName",
          "profileType"
        ]
      },
      "RegisterResponse": {
        "type": "object",
        "properties": {
          "token": {
            "type": "string",
            "description": "JWT access token",
            "example": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9..."
          },
          "user": {
            "description": "User data returned after successful registration",
            "example": {
              "id": 1,
              "email": "user@example.com",
              "avatar_url": "https://example.com/avatar.jpg"
            },
            "allOf": [
              {
                "$ref": "#/components/schemas/UserDto"
              }
            ]
          }
        },
        "required": [
          "token",
          "user"
        ]
      },
      "LoginDto": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "description": "Email пользователя"
          },
          "password": {
            "type": "string",
            "description": "Пароль пользователя",
            "minLength": 3
          }
        },
        "required": [
          "email",
          "password"
        ]
      },
      "LoginResponse": {
        "type": "object",
        "properties": {
          "token": {
            "type": "string",
            "description": "JWT access token",
            "example": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9..."
          },
          "user_id": {
            "type": "number",
            "description": "User id returned after successful registration",
            "example": 1
          }
        },
        "required": [
          "token",
          "user_id"
        ]
      }
    }
  }
}